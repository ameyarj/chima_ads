# SQLite database file (used by Prisma)
DATABASE_URL="file:./dev.db"

# LLM provider: openai, anthropic, etc.
LLM_PROVIDER=openai

# LLM model name (e.g., gpt-4.1)
LLM_MODEL=gpt-4.1

# API key for your LLM provider (e.g., OpenAI or Anthropic)
LLM_API_KEY=your_openai_api_key_here

# (Optional) Base URL for local or custom LLMs
# LLM_BASE_URL=http://localhost:1234/v1

# Server port
PORT=5000

# Node environment
NODE_ENV=development